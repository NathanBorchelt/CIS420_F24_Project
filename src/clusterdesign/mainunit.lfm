    object NodeTabSheet: TTabSheet
      Hint = 'Set constraints on characteristics of compute nodes,'#13#10'either manually (by providing a list of constraints) or'#13#10'automatically (by using a heuristic).'
      Caption = 'Nodes'
      ClientHeight = 523
      ClientWidth = 986
      object NodeLevelDesignConstraintsMemo: TMemo
        AnchorSideLeft.Control = NodeImposeDesignConstraintsButton
        AnchorSideTop.Control = NodeLevelDesignConstraintsLabel
        AnchorSideTop.Side = asrBottom
        AnchorSideRight.Control = NodeTabSheet
        AnchorSideRight.Side = asrBottom
        AnchorSideBottom.Control = NodeOutputMemo
        Left = 30
        Height = 149
        Hint = 'You can set constraints on any numeric characteristics'#13#10'defined in the database, such as the number of'#13#10'cores in a CPU, the amount of memory on the compute'#13#10'node, the cost of a compute node, etc.'#13#10#13#10'Prepend names of characteristics with "min_" and "max_".'#13#10'For example, "min_cpu_cores=12" only selects compute'#13#10'node configurations where the number of cores in a single'#13#10'CPU is 12 or more.'#13#10#13#10'Use "max_" and "min_" on the same characteristic to select'#13#10'a specific value for it. For example, "min_cpu_cores=12"'#13#10'and "max_cpu_cores=12" ensures that only configurations'#13#10'with exactly 12 cores are used -- not 8 or 16.'#13#10#13#10'To comment out a specific constraint temporarily, put'#13#10'the "#" sign before it.'#13#10#13#10'Constraints are supposed to limit the number of configurations'#13#10'that will be handled -- if you know what you are doing.'#13#10'If unsure, don''t impose constraints -- the tool will analyze'#13#10'all configurations, and choose the best for you.'#13#10#13#10'To revert the imposition of constraints, use the "Undo" button'#13#10'on the toolbar.'
        Top = 78
        Width = 342
        Anchors = [akTop, akLeft, akBottom]
        BorderSpacing.Top = 3
        BorderSpacing.Right = 30
        BorderSpacing.Bottom = 20
        Enabled = False
        Lines.Strings = (
          '#min_cpu_cores=12'
          '#min_node_cpu_count=2'
          '#min_node_main_memory_per_core=2'
          '#max_node_main_memory_per_core=2'
        )
        ScrollBars = ssAutoBoth
        TabOrder = 0
      end
