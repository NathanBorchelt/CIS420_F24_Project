    <!-- Include a configuration that has all 648 ports -->
    <include>mellanox_sx6536_648_ports_only.xml</include>
    
    <!-- All 36 leaf modules are sequentially connected to one another. But there are
         also configurations that use from 1 to 35 modules. Hence we connect the first 35
         leaf partitions directly to the "End" vertex. This way, configurations with 1..35 leaf
         modules also become possible. -->
    <connect from-partition="648_L1" to="End"/>
